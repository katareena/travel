{
  "name": "travel",
  "version": "1.0.0",
  "private": true,
  "description": "Грейдирование «Путешествие по Европе»",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "9.5.x",
    "browser-sync": "^2.26.12",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "gh-pages": "^2.2.0",
    "gulp": "4.0.x",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-csso": "^4.0.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-newer": "^1.4.0",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "8.0.x",
    "gulp-posthtml": "^3.0.4",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "2.6.x",
    "gulp-svgstore": "^7.0.1",
    "gulp-uglify": "^3.0.2",
    "gulp-webp": "^4.0.1",
    "posthtml-include": "^1.4.3",
    "stylelint": "10.1.x",
    "stylelint-config-htmlacademy": "0.1.x"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/scss/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "cross-env NODE_ENV=production npm start build",
    "start": "gulp",
    "deploy": "npm run build && gh-pages -d build"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/scss/**/*.scss"
  ],
  "engines": {
    "node": "8.16"
  },
  "dependencies": {
    "normalize-scss": "^7.0.1",
    "picturefill": "^3.0.3",
    "svg4everybody": "^2.1.9"
  }
}
